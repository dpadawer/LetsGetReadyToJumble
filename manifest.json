{
  "name": "Jumble",
  "version": "1.0",
  "description": "Jumble ALL the things!",
  "background": {"page": "background.html"},
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery-2.0.2.min.js", "background.js" ],
    "css": ["customStyles.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}